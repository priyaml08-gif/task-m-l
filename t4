from sklearn.ensemble import RandomForestClassifier

# Dummy features representing gestures
X = [[10, 20], [12, 22], [50, 60], [55, 65]]
y = ['Open Hand', 'Open Hand', 'Fist', 'Fist']

model = RandomForestClassifier()
model.fit(X, y)

prediction = model.predict([[11, 21]])
print("Predicted Gesture:", prediction[0])